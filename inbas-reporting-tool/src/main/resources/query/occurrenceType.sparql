PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX e: <http://onto.fel.cvut.cz/ontologies/eccairs-1.3.0.8/>
PREFIX m: <http://onto.fel.cvut.cz/ontologies/eccairs/model/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX j.0: <http://onto.fel.cvut.cz/ontologies/eccairs-1.3.0.8/>

CONSTRUCT {    
	?term rdfs:label ?label ;
	rdfs:comment ?comment ;
} WHERE {
	# Occurrence types
	e:A-430 e:hasChild  ?term.
	?term rdfs:label ?label.
	?term j.0:hasExplanation ?comment.
    BIND(m:occurrence-category as ?type)
}
