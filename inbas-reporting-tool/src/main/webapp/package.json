{
  "name": "inbas-reporting-tool",
  "version": "1.0.0",
  "description": "Reporting tool for the INBAS project.",
  "main": "js/app.js",
  "dependencies": {
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "react-addons-linked-state-mixin": "~0.14.7",
    "keymirror": "~0.1.0",
    "object-assign": "~4.0.1",
    "superagent": "~1.2.0",
    "react-router": "~1.0.3",
    "history": "~1.17.0",
    "react-bootstrap": "~0.27.3",
    "react-router-bootstrap": "~0.19.3",
    "body-parser": "~1.4.3",
    "express": "~4.4.5",
    "reflux": "~0.3.0",
    "kbss-react-bootstrap-datetimepicker": "https://dev.inbas.cz/dist/kbss-react-bootstrap-datetimepicker-0.0.3.tgz",
    "classnames": "~2.2.0",
    "react-bootstrap-typeahead": "https://dev.inbas.cz/dist/react-bootstrap-typeahead-0.0.2.tgz",
    "remarkable": "~1.6.0",
    "js-cookie": "~2.0.3",
    "halogen": "~0.1.10",
    "react-intl": "~2.0.0-rc-1",
    "jsonld": "~0.4.5",
    "react-pivot": "1.14.0",
    "react-d3": "0.4.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "uglify-js": "^2.6.1",
    "watchify": "^2.1.1",
    "clean-css": "^3.4.3",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "jasmine-reporters": "^2.0.7",
    "react-addons-test-utils": "^0.14.3",
    "babel-core": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babelify": "^7.2.0",
    "jsdom": "^7.0.2",
    "null-loader": "^0.1.1",
    "rewire": "^2.3.4",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "browserify": "browserify . -t [envify --NODE_ENV production] -o js/bundle.js",
    "build-js": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > ../../../profiles/production/js/bundle.min.js",
    "build-css": "cleancss -d -o ../../../profiles/production/resources/css/inbas-audit.min.css resources/css/inbas-audit.css",
    "build": "npm run build-css & npm run build-js",
    "prebuild": "npm install",

    "test": "jasmine JASMINE_CONFIG_PATH=tests/jasmine.json"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },

  "author": "Martin Ledvinka",
  "repository": {
    "type": "git",
    "url": "https://dev.inbas.cz/git/inbas-system"
  },
  "private": true
}
